{% extends 'base.html.twig' %}

{% block body %}
    {{ form_start(form) }}
    {{ form_errors(form) }}

    {% for formInput in form %}

        <div class="row">
                <div class="col-sm-3 col-lg-3">
                    {% if 'token' not in form_label(formInput) %}
                        {{ form_label(formInput) }}
                    {% endif %}
                    {{ form_errors(form) }}
                    {{ form_widget(formInput, {'attr': {'class': 'form-control','disabed':'true'}}) }}

                </div>
        </div>
    {% endfor %}
    {#{{ form_widget(form.save, {'attr': {'class': 'btn btn-primary','label':'falala'}}) }}#}

    {{ form_end(form) }}
{% endblock %}
